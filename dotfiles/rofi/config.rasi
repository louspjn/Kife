@import "./colors.rasi"

* {
    font: "Roboto 12";
    background-color: @primary-container;

    margin: 0px;
    padding: 0px;
    spacing: 0px;
}

window {
    location: north;
    y-offset: calc(50% - 176px);
    width: 480;
    border-radius: 24px;
}

inputbar {
    border: 2px;
    border-radius: 16px;
}

entry {
    placeholder: "Search";
}
